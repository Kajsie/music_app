<%= form_for([@playlist, @playlist.songs.build]) do |f| %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= f.label :link %>
    <%= f.text_field :link %>
  </p>
  <p>
    <%= f.label :votes %>
    <%= f.number_field :votes %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
